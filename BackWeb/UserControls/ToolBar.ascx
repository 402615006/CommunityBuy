<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="ToolBar.ascx.cs" Inherits="CommunityBuy.BackWeb.UserControls.ToolBar" %>
<div class="operbtn" id="ToolBarMenu" runat="server">
    <asp:LinkButton ID="LinkRefresh" runat="server" CommandName="refresh" OnClick="ToolBar_Click" Visible="true"><div class="operlinkbtn"><img src="/img/operations/again.png" /><span data-code="refresh_button" class="fontcolor">刷新</span></div></asp:LinkButton>
    <asp:LinkButton ID="LinkSearch" runat="server" CommandName="search" OnClick="ToolBar_Click"><div class="operlinkbtn"><img src="/img/operations/select.png" /><span data-code="search_button" class="fontcolor">查询</span></div></asp:LinkButton>
        <asp:LinkButton ID="LinkInfo" runat="server" CommandName="info" OnClick="ToolBar_Click" OnClientClick="return checklist('info');" Visible="true"><div class="operlinkbtn"><img src="/img/operations/update.png" /><span data-code="info_button" class="fontcolor">详情</span></div></asp:LinkButton>
    <asp:LinkButton ID="LinkAdd" runat="server" CommandName="add" OnClick="ToolBar_Click" OnClientClick="return checklist('add');" Visible="true"><div class="operlinkbtn"><img src="/img/operations/add.png" /><span data-code="add_button" class="fontcolor">添加</span></div></asp:LinkButton>
