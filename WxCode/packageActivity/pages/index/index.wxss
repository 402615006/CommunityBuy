}

luckyDraw {
  width: 100%;
}

/* 头部样式 */

.luckyDraw .headerbg {
  width: 100%;
  height: 280rpx;
}

.headerbg1 {
  width: 100%;
  height: 380rpx;
}

.luckyDraw .pageBg image.second {
  position: relative;
  /* z-index: 99; */
}

/* 主体样式 */

.luckyDraw .main {
  width: 100%;
  background-color: #ea512f;
  padding-bottom: 30rpx;
}

.prizeContent-box {
  position: relative;
  overflow: hidden;
}

.prizeContent-box image.second {
  position: relative;
  z-index: 9;
  width: 100%;
  display: block;
  height: 700rpx;
}

.luckyDraw .main .prizeContent {
  width: 560rpx;
  height: 240rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  position: absolute;
  left: 50%;
  top: 90rpx;
  z-index: 36;
  transform: translate(-50%);
}

.prizeBox {
  transform-style: preserve-3d;
  -webkit-transform-style: preserve-3d;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  transform-origin: 50% 50%;
  animation-timing-function: ease;
}

.luckyDraw .main .prizeContent .prizeList {
  width: 200rpx;
  height: 240rpx;
  position: relative;
  /* #dcdada, #fefefe, #dcdada */
  background-image: linear-gradient(#fff, #fff);
  transform-style: preserve-3d;
  -webkit-transform-style: preserve-3d;
}

@keyframes autoMove {
  from {}

  to {
    transform: rotateX(720deg);
  }
}

.luckyDraw .main .prizeContent .prizeList .red-envelope {
  width: 160rpx;
  position: absolute;
  transform-origin: center;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* .luckyDraw .main .prizeContent .prizeList .red-envelope:nth-of-type(3) {} */

.luckyDraw .main .prizeContent .prizeList .red-envelope image {
  display: block;
  width: 160rpx;
}

.luckyDraw .main .prizeContent .prizeList .red-envelope text {
  font-size: 26rpx;
  color: #ffe04e;
  position: absolute;
}

/* 按钮 */

.btn-box {
  position: relative;
}

.luckyDraw .main .btnimg {
  width: 300rpx;
  height: 70rpx;
  position: absolute;
  left: 50%;
  bottom: 260rpx;
  z-index: 35;
  transform: translate(-50%);
}

.luckyDraw .main .btnimg1 {
  width: 80rpx;
