/* 头部滚动 */

.top {
  width: 100%;
  height: 120rpx;
  overflow: hidden;
  position: relative;
}

.ul {
  height: 100rpx;
  white-space: nowrap;
  position: absolute;
  top: 20rpx;
  display: flex;
}

.prizerList1 {
  display: flex;
}

.prizerList2 {
  display: flex;
}

.lis {
  margin-right: 60rpx;
  height: 60rpx;
  /* min-width: 200rpx; */
  padding: 0 20rpx;
  border-radius: 30rpx;
  text-align: center;
  font-size: 24rpx;
  color: #f6e23e;
  line-height: 60rpx;
  box-shadow: 0px 0px 30rpx #760b72 inset;
  display: flex;
}

.user_img {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 20rpx;
  margin-top: 10rpx;
}

.lis:nth-child(2n) {
