.buton_img{
  width: 67rpx;
  height: 67rpx;
  border-radius: 50%;
  position: relative;
}
.buton_img image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left:50%;
  top:50%;
  transform: translate(-50%,-50%)
}
.button_text{
  font-size: 26rpx;
  margin-top:10rpx;
  /* width: 82rpx; */
}


.coupon{
  width: 690rpx;
  height: 60rpx;
  margin: auto;
  background-color: #eae9e9;
  border-radius: 6rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.coupon_text{
  font-size: 20rpx;
  color:#a7a6a6;
  font-weight: bold;
}
.btn_coupon{
  width: 90rpx;
  height: 44rpx;
  border-radius: 10rpx;
  background-color: #ea6248;
  color:#fff;
  font-size: 22rpx;
  padding:0 10rpx;
  line-height: 44rpx;
}
.coupon_icon{
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  background-color: #fdc14d;
  font-size: 34rpx;
  color:#fff;
  margin-right:18rpx;
  margin-left:20rpx;
}
.jiantou{
  width: 50rpx;
  height: 50rpx;
}

.recommended{
  width: 100%;
  padding:20rpx 0;
  background-color: #fff;
  margin-top:20rpx;
}
.header{
  padding:0 20rpx;
  box-sizing: border-box;
}
.header .title{
  font-size: 30rpx;
  color:#000;
  font-weight: 600;
}
.header .text{
  font-size: 24rpx;
  color:#575757;
  margin-top:20rpx;
}

.scrolls{
  width: 100%;
  /* white-space: nowrap; */
  margin-top:20rpx;
  /* padding-bottom:5rpx; */
}
.recommended_box{
  width: 100%;
  white-space: nowrap;
  padding-bottom:5rpx;
}
.scrolls .li{
  width: 278rpx;
  border-radius: 10rpx;
  overflow: hidden;
  box-shadow: 0 0 10px #cbc6c6;
  position: relative;
  display: inline-block;
  margin-right:20rpx;
}
.scrolls .li .shopp_icon{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  bottom:10rpx;
  right:10rpx;
}
.scrolls .li:nth-child(1){
  margin-left:20rpx;
}
.scrolls .li image{
  width: 278rpx;
  height: 248rpx;
}
.scrolls .li .name{
  width: 100%;
  padding-left:5rpx;
  box-sizing: border-box;
  height: 56rpx;
  font-size: 30rpx;
  line-height: 56rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.scrolls .li .num{
  width: 100%;
  padding-left:5rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  line-height: 40rpx;
  color:#a2a1a1;
}
.scrolls .li .price{
  width: 100%;
  padding-left:5rpx;
  box-sizing: border-box;
  font-size: 18rpx;
  line-height: 40rpx;
  color:#ea6248;
}
.scrolls .li .price_num{
  font-size: 32rpx;
  font-weight: 600;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
