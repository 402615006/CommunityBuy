.head{
  width: 700rpx;
  border-radius: 16rpx;
  margin:auto;
  margin-top:20rpx;
  padding:20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.head .title{
  font-size: 30rpx;
  font-weight: 600;
  line-height: 40rpx;
}
.head .text{
  display: flex;
  justify-content: flex-start;
  margin-top:10rpx;
}
.head .text .xing{
  width:34rpx;
  height: 34rpx;
  margin-right:3rpx;
}
.head_price{
  font-size: 22rpx;
  color:#575757;
  margin-left:18rpx;
}
.junjia{
  color:#f42323;
}
.banner{
  margin-top:10rpx;
  height: 240rpx;
  width: 100%;
  border-radius: 10rpx;
  overflow: hidden;
}
swiper{
  height: 240rpx;
  width: 100%;
}
.banner-item{
  width: 100%;
  height: 240rpx;
}

.head_content{
  padding:0 10rpx;
  box-sizing: border-box;
}

.status{
  display: flex;
  justify-content: flex-start;
  padding:20rpx 0;
}
.stauts .status_left{

}
.status .status_right{
  padding-left:5rpx;
  box-sizing: border-box;
}
.status .shangpu{
  width: 40rpx;
  height: 40rpx;
}
.status_top{
  font-size: 22rpx;
  color:#000;
  font-weight: bold;
}
.margin_right10{
  margin-right:10rpx;
}
.color_000000{
  color:#000000;
}
.status_bottom .block{
  height: 32rpx;
  border: 1rpx solid #f9d0c8;
  display: inline-block;
  font-size: 18rpx;
  color:#ea6248;
  line-height: 31rpx;
  padding:0 12rpx;
  margin-right:10rpx;
}

/* 标签 */
.label{
  width: 100%;
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.label_box{
  height: 40rpx;
  line-height: 40rpx;
  max-width:500rpx;    
  overflow: hidden;    
  text-overflow:ellipsis;    
  white-space: nowrap;
}
.lable_text{
  color:#a7a6a6;
  font-size: 22rpx;
  margin-right:20rpx;
}
.icon_jiantou{
  width: 40rpx;
  height: 40rpx;
}

.address{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:10rpx 0 20rpx 0;
}
.address .address_right{
  width: 84rpx;
  height: 50rpx;
  /* border-left: 1rpx solid #888888; */
  text-align: right;
}
.address .tel{
  width: 50rpx;
  height: 50rpx;
  /* margin-top:-7rpx; */
}

.address_left{
  display: flex;
  justify-content: flex-start;
}
.add_leftone{
  width: 40rpx;
  height: 40rpx;
}
.addimg{
  width: 40rpx;
  height: 40rpx;
}
.add_text{
  max-width: 500rpx;
  
}
.addname{
  color:#000;
  font-size:22rpx;
  font-weight: bold;
  line-height: 30rpx;
  position: relative;
  padding-left:30rpx;
  box-sizing: border-box;
}

.address .s{
  font-size: 22rpx;
  color:#a7a6a6;
  margin-top:20rpx;
  font-weight: bold;
  
}
.icon_weizhi{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left:-5rpx;
  top:0;
}



