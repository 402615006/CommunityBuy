page {
  background-color: #f7f7f7;
}

.groupdetail {
  padding-bottom: 130rpx;
  box-sizing: border-box;
}

.head {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
  align-items: center;
}

.head_left {
  display: flex;
  justify-content: flex-start;
  height: 90rpx;
  align-items: center;
}

.title {
  font-size: 28rpx;
  line-height: 34rpx;
  font-weight: 600;
}

.jiantou {
  width: 50rpx;
  height: 50rpx;
  margin-left: 10rpx;
}

.status {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  color: #9b9a9a;
}

.status .left {
  font-size: 20rpx;
  display: flex;
  align-items: center;
}

.status .right {
  font-size: 22rpx;
}

.gou {
  width: 30rpx;
  height: 30rpx;
}

.icon {
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
  border: 1rpx solid #8b8b8b;
  border-radius: 12rpx;
}

.banner {
  width: 100%;
  height: 260rpx;
  /* border-radius: 10rpx; */
  overflow: hidden;
}

swiper {
  height: 260rpx;
}

swiper-item {
  overflow: hidden;
  height: 260rpx;
  /* border-radius: 10rpx; */
}

.banner-item {
  width: 100%;
  height: 260rpx;
  padding: 0 20rpx;
  /* border-radius: 10rpx; */
}

.box {
  padding: 0 0 30rpx 0;
  box-sizing: border-box;
}

.ul {
  margin-bottom: 20rpx;
}

.ul .name {
  font-size: 28rpx;
  font-weight: 600;
}

.ul .li {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  margin-top: 15rpx;
}

.ul .number {
  font-size: 22rpx;
  color: #9b9a9a;
  margin-left: 10rpx;
}

/* 说明 */

.description {
  width: 100%;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  background-color: #fff;
}

.description .header {
  font-size: 28rpx;
  font-weight: 600;
  height: 50rpx;
  line-height: 50rpx;
}

.description .title_name {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #9b9a9a;
}

.description .text {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #13161a;
}

