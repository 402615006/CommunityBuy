.content {
  padding: 0 16rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}

.content_title {
  height: 100rpx;
  line-height: 100rpx;
}

.content_li {
  width: 100%;
  min-height: 88rpx;
  display: flex;
  justify-content: space-between;
  border-top: 1rpx solid #e6e6e6;
  align-items: center;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.content_left {
  width: 60%;
}

.content_right {
  width: 40%;
  display: flex;
  justify-content: space-between;
}

.font_weight {
  font-weight: 600;
}

.font_size24 {
  font-size: 24rpx;
}

.color_838383 {
  color: #838383;
}

.color_EA {
  color: #ea6349;
}

.offer {
  padding: 0 16rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}

.offer_li {
  width: 100%;
  min-height: 100rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  align-items: center;
  padding-right: 20rpx;
  box-sizing: border-box;
  color: #000;
}

.offer_li:nth-child(2) {
  border-top: 1rpx solid #e6e6e6;
}

.offer_li:nth-child(3) {
  border-top: 1rpx solid #e6e6e6;
}

.offer_right {
  display: flex;
  justify-content: flex-start;
}

.footer {
  width: 100%;
  height: 100rpx;
  border-top: 1rpx solid #e6e6e6;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0 26rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  padding-bottom: constant(safe-area-inset-bottom);
}

