/* packageFood/pages/grorderdetail/grorderdetail.wxss */
page {
  background-color: #f7f7f7;
  padding-bottom: 130rpx;
}
.groupdetail {
  padding-bottom: 120rpx;
  box-sizing: border-box;
}

.head {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
  align-items: center;
}

.head_left {
  display: flex;
  justify-content: flex-start;
  height: 90rpx;
  align-items: center;
}
.title {
  font-size: 28rpx;
  line-height: 34rpx;
  font-weight:600;

}
.jiantou {
  width: 50rpx;
  height: 50rpx;
  margin-left: 10rpx;
}
.jiantou1{
  width: 64rpx;
  height: 64rpx;
  margin-left: 10rpx;
}
.status {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  color: #9b9a9a;
}

.status .left {
  font-size: 20rpx;
  display: flex;
  align-items: center;
}

.status .right {
  font-size: 22rpx;
}

.gou {
  width: 30rpx;
  height: 30rpx;
}

.icon {
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
  border: 1rpx solid #8b8b8b;
  border-radius: 12rpx;
}

.banner {
  width: 100%;
  height: 260rpx;
  /* border-radius: 10rpx; */
  overflow: hidden;
}

swiper {
  height: 260rpx;
}

swiper-item {
  overflow: hidden;
  height: 260rpx;
  /* border-radius: 10rpx; */
}

.banner-item {
  width: 100%;
  height: 260rpx; 
  padding:0 20rpx;
/* border-radius: 10rpx; */
}

.box {
  padding: 0 0 30rpx 0;
  box-sizing: border-box;
}

.ul {
  margin-bottom: 20rpx;
}

.ul .name {
  font-size: 28rpx;
  font-weight: 600;
}

.ul .li {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  margin-top: 15rpx;
}

.ul .number {
  font-size: 22rpx;
  color: #9b9a9a;
  margin-left: 10rpx;
}

/* 说明 */

.description {
  width: 100%;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  background-color: #fff;
}

.description .header {
  font-size: 28rpx;
  font-weight: 600;
  height: 50rpx;
  line-height: 50rpx;
}

.description .title_name {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #9b9a9a;
}

.description .text {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #13161a;
}

.color_eaab48 {
  background-color: #eaab48;
}

.color_ea6248 {
  background-color: #ea6248;
}

.font_size20 {
  font-size: 20rpx;
}

.font_size24 {
  font-size: 24rpx;
}

.margin_top10 {
  margin-top: 10rpx;
}

.margin_top5 {
  margin-top: 5rpx;
}

.margin_left5 {
  margin-left: 5rpx;
}

.margin_left10 {
  margin-left: 10rpx;
}
.margin_left20 {
  margin-left: 20rpx;
}
.content {
  padding: 0 16rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}

.content_title {
  height: 100rpx;
  line-height: 100rpx;
}

.content_li {
  width: 100%;
  min-height: 88rpx;
  display: flex;
  justify-content: space-between;
  border-top: 1rpx solid #e6e6e6;
  align-items: center;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.content_left {
  width: 60%;
}

.content_right {
  width: 40%;
  display: flex;
  justify-content: space-between;
}

.font_weight {
  font-weight: 600;
}

.font_size24 {
  font-size: 24rpx;
}

.color_838383 {
  color: #838383;
}

.color_EA {
  color: #EA6349;
}

.offer {
  padding: 0 16rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 30rpx;
  margin-top: 30rpx;
  background-color: #fff;
}

.offer_li {
  width: 100%;
  min-height: 100rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  align-items: center;
  padding-right: 20rpx;
  box-sizing: border-box;
  color: #000;
}

.offer_li:nth-child(2) {
  border-top: 1rpx solid #e6e6e6;
}

.offer_li:nth-child(3) {
  border-top: 1rpx solid #e6e6e6;
}
.offer_li:nth-child(4) {
  border-top: 1rpx solid #e6e6e6;
}

.offer_right {
  display: flex;
  justify-content: flex-start;
}
.footer {
  width: 100%;
  height: 100rpx;
  border-top: 1rpx solid #e6e6e6;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0 26rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  z-index: 2;
  padding-bottom: constant(safe-area-inset-bottom);
}
.footer1 {
  width: 100%;
  height: 110rpx;
  border-top: 1rpx solid #e6e6e6;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0 26rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #333;
  z-index: 12;
}

