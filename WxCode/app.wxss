/* 登录 */
.login_bj{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2000;
}
.login_icon0{

  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 20rpx;
  top: 30rpx;
}
.login_img{
  position: fixed;
  width: 640rpx;
  height:  700rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  z-index: 33;
}
.login_content{
  position: fixed;
  width: 640rpx;
  height:  700rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
  z-index: 44;
}
.login_top{
  margin-top: 90rpx;
}
.login_icon{
  width: 50rpx;
  height: 40rpx;
  position: absolute;
  left: 0;
  bottom: 20rpx;
}

.login_icon1{
  width: 60rpx;
  height: 40rpx;
  position: absolute;
  left: 0;
  bottom: 20rpx;
}
.login_text{
  width: 500rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #dedede;
  display: flex;
  margin: 0 auto;
  position: relative;
  margin-top: 20rpx;
}

.login_input{
  width: 200rpx;
  height: 70rpx;
   position: absolute;
  left: 80rpx;
  bottom: 10rpx;
  text-align: left;
  font-size: 30rpx;
}
.login_btn{
  width: 500rpx;
  height: 100rpx;
  border-radius: 50rpx;
  line-height: 100rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.weixinlogin{

  width: 500rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 auto;
  margin-top: 30rpx;


}
.login_code{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  position: absolute;
  right: 0;
  bottom: 10rpx;
}
